{
  "name": "styleguide",
  "version": "0.0.1",
  "description": "deBijenkorf jekyll styleguide",
  "main": "_config.yml",
  "dependencies": {},
  "devDependencies": {
    "release": "^1.1.5"
  },
  "scripts": {
    "build": "bundle exec jekyll build",
    "serve": "bundle exec jekyll serve -w --config _config.yml,_config.dev.yml",
    "release:patch": "npm version patch && release",
    "release:minor": "npm version minor && release",
    "release:major": "npm version major && release",
    "postversion": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deBijenkorf/styleguide.git"
  },
  "author": {
    "company": "Magazijn de Bijenkorf",
    "email": "developers@debijenkorf.nl",
    "url": "http://www.debijenkorf.nl/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deBijenkorf/styleguide/issues"
  }
}
